#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

float vol_cilindro(float r, float h);


void main() 
 {
    float radio, altura, volumen;	
    cout <<"Ingrese el radio:" ; 
	cin >>radio;	
    cout <<"Ingrese la altura:"; 
	cin >> altura;	
    volumen= vol_cilindro(radio,altura);
    cout <<"El volumen es:" <<volumen;    
    getch();
 }

float vol_cilindro (float r, float h)
 { float v= 3.141516*r*r*h ;	
    return v;    
}
